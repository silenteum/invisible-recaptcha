{
    "name": "silenteum/invisible-recaptcha",
    "description": "Invisible reCAPTCHA For Laravel.",
    "keywords": ["recaptcha", "no-captcha", "captcha", "invisible", "invisible-recaptcha", "laravel", "laravel5", "php"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.* || ^7.0",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "illuminate/view": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Silenteum\\InvisibleReCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silenteum\\InvisibleReCaptcha\\InvisibleReCaptchaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
